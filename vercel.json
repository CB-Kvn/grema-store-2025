{
    "version": 2,
    "rewrites": [
      {
        "source": "/:path*",
        "destination": "/index.html"
      }
    ],
    "routes": [
      {
        "src": "/static/:path*",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/:path*",
        "dest": "/index.html"
      }
    ]
  }